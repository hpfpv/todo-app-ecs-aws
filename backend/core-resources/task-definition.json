{
    "family": "todohouessoucomecsservice",
    "cpu": "256",
    "memory": "512",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "EC2"
    ],
    "executionRoleArn": "arn:aws:iam::601091111123:role/todo-houessou-com-ecs-EcsServiceRole-1SD3VZ4W9UVY9",
    "taskRoleArn": "arn:aws:iam::601091111123:role/todo-houessou-com-ecs-ECSTaskRole-118HY0AV42A5E",
    "containerDefinitions": [
      {
        "name": "todo-houessou-com-ecs-Service",
        "image": "601091111123.dkr.ecr.us-east-1.amazonaws.com/todo-houessou-com-ecs/todo-main-service:latest",
        "portMappings": [
          {
            "containerPort": 8080,
            "protocol": "http"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "todo-houessou-com-ecs-logs",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "todo-houessou-com-ecs-service"
          }
        },
        "essential": true
      }
    ]
  }